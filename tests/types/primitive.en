// Primitive types and operations

// Basic assignments
var int1 = 100
var int2 int = 100

var float1 = 123.456
var float2 float = 123.456

var string1 = "enact"
var string2 String = "enact"

var bool1 = true
var bool2 bool = true

// Implicit conversions
var float3 = int1

// Unary operations
bool2 = !bool2
int2 = -int2
float3 = -float3

// Binary operations
int1 = int1 + int2

float1 = float1 + float2
float2 = int1 + float2

string1 = string1 + string2

int1 = int1 - int2
float1 = float1 - float2
float2 = int1 - float2

int1 = int1 * int2
float1 = float1 * float2
float2 = int1 * float2

int1 = int1 / int2
float1 = float1 / float2
float2 = int1 / float2

bool1 = int1 == int2
float1 == int2
int2 == float1